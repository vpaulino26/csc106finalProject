var banker = function(){

    background(0, 0, 0);
    textSize(24);
    fill(255, 208, 0);
    var bank = [];
    var j = 0;
    for (var i = 5; i > 0; i--){
        bank.push(round(total/i));
    }
/*
    println(total);
    println(bank[0]);
    println(bank[1]);
    println(bank[2]);
    println(bank[3]);
    println(bank[4]);
*/
    
    var deal = new Button ({
    x: 65,
    y: 300,
    color: color(255, 208, 0),
    label: "Deal",
    width: 95,
    onClick: function() {
        currentScene = 3;
        total = bank[j];
    }
});


    text("The Banker wants to make and offer",2,49); 
    text("The offer is $" + round(bank[j]),87,132);  
    deal.draw();
    noDeal.draw();
    drawBitmoji(26,149,60);
    strokeWeight(5);
    stroke(255, 255, 255);
    line(85,228,186,150);
    noFill();
    rect(81,99,230,50);
    if (shifter === 1)
    {
        j += 1;
        shifter = 0;
    }
    else
    {
        total = bank[j];
    }
    
    
};
banker();

draw = function() {
    fill(255, 255, 255);
    rect(240,368,152,33);
    fill(0, 0, 0);
textSize(15);
text("Boxes Eliminated: " + score, 250, 380);
if (currentScene === 0) {
        animate++;
        vicMoves = vicMoves +2;
        drewMoves = drewMoves -2;
        background(117, 178, 221);
        textSize(37);
        fill(0 + animate, 0 + animate, 0 + animate);
        text("DEAL OR NO DEAL?", 20, 30);
        drawBitmoji(-110 +vicMoves,70,69);
        drawbitmoji(350 +drewMoves,65,78);
        startGame.draw();
        startBox.draw();
        startBox.x = startBox.x +3;
        if (startBox.x > 420) {
            startBox.x =-90;
        }
        if (animate > 200) {
            animate = 0;
        }
        if (vicMoves >500) {
            vicMoves = 0;
            drawBitmoji(-110 +vicMoves,70,69);
        }
        if (drewMoves <-500) {
            drewMoves = 0;
            drawbitmoji(350 +drewMoves,65,78);

        }
    }
    else if (currentScene === 1) {
        main();
    }
};
