var box1 = new Button ({
    x: 26,
    y: 14,
    width: 117,
    box:true,
    NumBox: 1,
    mon: boxMoney[0],
    onClick: function() {
        fill(0, 0, 0);
        rect(box1.x,box1.y,79,40);
        fill(255, 255, 255);
        text(box1.mon,box1.x,box1.y+11,79,50);
        total -= boxMoney[box1.NumBox-1];
        score-=1;
        
    }
});

