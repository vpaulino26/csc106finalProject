//score global variable
var score = 6;
var boxMoney = [];
var currentScene = 0;
var total = 0;
var holder = 0;
var shifter = 0;
var animate = 0;
var vicMoves = 0;
var drewMoves = 0;





var money = [0,1,10,20,40,50,60,80,100,200,250,300,350,400,450,500,550,1000,1500,5000,5500,10000,10500,50000,100000,500000,1000000];


//randomizes the order of the mpney in the boxes
for (var j = 0; j < money.length; j++)
{

        holder = round(random(1,27));
        if (money[holder] === 0)
        {
            while (money[holder] === 0)
            {
                if (holder === 27)
                    {
                        holder = 1;
                    }
                else
                {
                    holder += 1;
                }
            }
        }
    else if (money[holder] > 0)
    {
        boxMoney.push(money[holder]);
        money[holder] = 0;
    }


}

for(var c = 0; c < money.length; c ++)
{
    if (money[c] > 0)
        {
            boxMoney.push(money[c]);
            
        }
}



for (var i = 0; i < money.length; i++)
{
    total += money[i]; 
    
}

//println(total);

//println(money.length);


var Button = function(config) {
    this.x = config.x || 0;
    this.y = config.y || 0;
    this.color = config.color || color(0, 238, 255);
    this.width = config.width || 80;
    this.height = config.height || 50;
    this.label = config.label || "Click";
    this.onClick = config.onClick || function() {};
    this.box = config.box || false;
    this.NumBox = config.NumBox || "00";
    this.mon = config.mon;

};

Button.prototype.draw = function() {

    if (this.box === true)
        {
            fill(143, 143, 143);
            rect(this.x,this.y,79,41);
            rect(this.x+15,this.y-12,50,12);
            fill(0, 123, 255);
            rect(this.x+24,this.y-7,33,7);
            fill(143, 143, 143);
            for(var i = 0; i < 7;i++)
                {
                 line(this.x+10+i*10,this.y+40,this.x+10+i*10,this.y);   
                }
            rect(this.x+20,this.y+12,40,20);
            fill(255,255,255);
            text(this.NumBox,this.x+33,this.y+12);
            
            

        }
    else
    {
        fill(this.color);
        rect(this.x, this.y, this.width, this.height, 5);
        fill(0, 0, 0);
        textSize(19);
        textAlign(LEFT, TOP);
        text(this.label, this.x+10, this.y+this.height/4);
    }
};



Button.prototype.isMouseInside = function() {
    return mouseX > this.x &&
           mouseX < (this.x + this.width) &&
           mouseY > this.y &&
           mouseY < (this.y + this.height);
};

Button.prototype.handleMouseClick = function() {
    if (this.isMouseInside()) {
        this.onClick();
    }
};
